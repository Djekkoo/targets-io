{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "graphiteui"
  },
  "spec": {
    "ports": [
      {
        "port": 80,
          "targetPort": "graphite-ui"
      }
    ],
    "selector": {
      "name": "graphite-ui"
    },
    "sessionAffinity": "ClientIP",
    "type": "LoadBalancer"
  }
}
